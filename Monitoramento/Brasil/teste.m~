clear;
clc;
data = readtable('arquivo_geral.csv', 'HeaderLines',1)

BRASIL = [data(:,4),data(:,5),data(:,6), data(:,7), data(:,2)];

idAC = find(strcmp([BRASIL{:,5}], 'AC'));
n=1;
n_end=max(size(idAC));
for (i = idAC(1,1):1:idAC(n_end))
AC(n,:) = BRASIL(i,:);
n=n+1;
end
idAP = find(strcmp([BRASIL{:,5}], 'AP'));
n=1;
n_end=max(size(idAP));
for (i = idAP(1,1):1:idAP(n_end))
